<?xml version="1.0"?>
<package format="2">
  <name>tgw_gazebo</name>
  <version>1.2.0</version>
  <description>
    Gazebo simulation package for the TurtleBot3 and TGW-Hackathon
  </description>
  <license>Apache 2.0</license>
  <author email="raimund.edlinger@fh-wels.at">Raimund Edlinger</author>
  <maintainer email="raimund.edlinger@fh-wels.at">Raimund Edlinger</maintainer>

  <buildtool_depend>catkin</buildtool_depend>
  <depend>roscpp</depend>
  <depend>std_msgs</depend>
  <depend>sensor_msgs</depend>
  <depend>geometry_msgs</depend>
  <depend>nav_msgs</depend>
  <depend>tf</depend>
  <depend>gazebo_ros</depend>
  <exec_depend>gazebo</exec_depend>
</package>
